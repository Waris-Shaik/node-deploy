{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],

    "routes":[
        {
            "src": "/api/v1/products",
            "dest": "index.js"
        },
          {
            "src": "/api/v1/products/(.*)",
            "dest": "index.js"
          },
          {
            "src": "/(.+)",
            "dest": "build/$1"
          },
          {
            "src":"/",
            "dest":"build/index.html"
          }
    ]
  
  }